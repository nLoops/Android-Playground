<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/home_fragment">

    <fragment
        android:id="@+id/home_fragment"
        android:name="co.eware.gists.base.HomeFragment"
        android:label="@string/home_fragment_label"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_home_fragment_to_observerFragment"
            app:destination="@id/observerFragment" />
    </fragment>
    <fragment
        android:id="@+id/observerFragment"
        android:name="co.eware.gists.categories.core.patterns.observer.ui.ObserverFragment"
        android:label="ObserverFragment"
        tools:layout="@layout/fragment_observer" />
</navigation>